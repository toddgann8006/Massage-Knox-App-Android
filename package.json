{
  "name": "massage-knox",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "build": "react-native bundle --platform android --dev false --entry-file index.js  --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/ && rm -rf ./android/app/src/main/res/drawable-mdpi/  && rm -rf ./android/app/src/main/res/raw/",
    "release": "yarn build && cd ./android && ./gradlew bundleRelease"
  },
  "dependencies": {
    "@react-native-community/masked-view": "0.1.10",
    "@react-native-firebase/app": "^14.3.0",
    "@react-native-firebase/messaging": "^14.3.0",
    "expo": "41",
    "expo-barcode-scanner": "~10.1.2",
    "expo-cli": "4",
    "expo-constants": "~10.1.3",
    "expo-permissions": "~12.0.1",
    "expo-splash-screen": "~0.10.2",
    "expo-status-bar": "~1.1.0",
    "expo-updates": "~0.5.4",
    "font-awesome": "^4.7.0",
    "google-maps-react": "2.0.6",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-native": "~0.63.4",
    "react-native-dotenv": "^3.3.1",
    "react-native-elements": "2.2.1",
    "react-native-gesture-handler": "~1.10.2",
    "react-native-input-validation": "^1.2.2",
    "react-native-push-notification": "3.1.9",
    "react-native-reanimated": "~2.1.0",
    "react-native-safe-area-context": "3.2.0",
    "react-native-screens": "~3.0.0",
    "react-native-swiper": "^1.6.0",
    "react-native-unimodules": "~0.13.3",
    "react-native-web": "0.17.1",
    "react-navigation": "4",
    "react-navigation-drawer": "^2.7.1",
    "react-navigation-stack": "^1.10.3",
    "react-navigation-tabs": "^2.11.2",
    "react-redux": "7.2.0",
    "redux": "4.0.5",
    "redux-logger": "3.0.6",
    "redux-persist": "5.9.1",
    "redux-thunk": "2.3.0",
    "unimodules-permissions-interface": "^6.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9"
  },
  "private": true
}